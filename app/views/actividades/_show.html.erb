<%- model_class = Actividad -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<div class='programa'>
 
  <p><b>Nombres: </b><%= @actividad.nombre %></p>
  <p><b>Pograma: </b><%= @actividad.tema.unidad.programa.nombre %></p>
  <p><b>Unidad: </b><%= @actividad.tema.unidad.nombre %></p>
  <p><b>Tema: </b><%= @actividad.tema.nombre %></p>
  <p><b>Descripcion: </b><%= @actividad.descripcion %></p>
 
</div>
