<%- model_class = Evaluacion -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize %></h1>
</div>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Archivo</th>
      <th>Nombre</th>
      <th>Calificacion</th>
      <th><%=t '.acciones', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <%= render @evaluaciones %>
  </tbody>
</table>

<tfoot>
  <td>
   <%= link_to "Nuevo evaluacion", new_evaluacion_path, :remote =>true, :class =>'btn btn-success' %>
 </td>
</tfoot>