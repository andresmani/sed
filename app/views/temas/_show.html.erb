<%- model_class = Tema -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<div class='programa'>
 
  <p><b>Tema: </b><%= @tema.nombre %></p>
  <p><b>Programa: </b><%= @tema.unidad.programa.nombre %></p>
  <p><b>Unidad: </b><%= @tema.unidad.nombre %></p>
  <p><b>Descripcion: </b><%= @tema.descripcion %></p>

 
</div>
